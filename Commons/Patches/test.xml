<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[race/intelligence = "Humanlike" and not(race/foodType = "None")]/recipes/li[text() = "CureScaria"]</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[race/intelligence = "Humanlike" and not(race/foodType = "None"]/recipes</xpath>
			<value>
				<li>CureScaria</li>
			</value>
		</nomatch>
	</Operation>
</Patch>